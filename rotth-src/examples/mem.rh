include "../std.rh"

const ALPHABET: u64 do
    'z' cast u64 'a' cast u64 - 1 +
end

mem foo do ALPHABET end

proc main: u64 do
    'a'
    0 while dup ALPHABET < do
        bind c: char i: u64 do
            c cast u64 i + cast u8
            foo i ptr+
            !u8
            c i
        end
        1 +
    end
    drop drop
    ALPHABET foo cast &>char puts
    0
end