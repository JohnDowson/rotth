  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'rotth'",
      "cargo": {
        "args": ["build", "--bin=rotth", "--package=rotth"],
        "filter": {
          "name": "rotth",
          "kind": "bin"
        }
      },
      "args": ["rotth-src/examples/count.rh"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
